                class="ai-kulkid-contact__form-input-{{ ai_gen_id }}"
                value="{{ form.order_number }}"
                placeholder="f.eks. #1001"
              >
            </div>

            <div class="ai-kulkid-contact__form-field-{{ ai_gen_id }}">
              <label for="contact-subject-{{ ai_gen_id }}" class="ai-kulkid-contact__form-label-{{ ai_gen_id }}">
                Velg emne *
              </label>
              <select
                id="contact-subject-{{ ai_gen_id }}"
                name="contact[subject]"
                class="ai-kulkid-contact__form-select-{{ ai_gen_id }}"
                required
                aria-required="true"
