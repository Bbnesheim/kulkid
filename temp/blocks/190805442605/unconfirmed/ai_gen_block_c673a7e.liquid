          <path d="M14 9a2 2 0 0 1-2 2H6l-4 4V4c0-1.1.9-2 2-2h8a2 2 0 0 1 2 2v5Z"/>
          <path d="M18 9h2a2 2 0 0 1 2 2v11l-4-4h-6a2 2 0 0 1-2-2v-1"/>
        </svg>
        {{ block.settings.form_title }}
      </h2>
      <p class="ai-kulkid-contact__form-subtitle-{{ ai_gen_id }}">{{ block.settings.form_subtitle }}</p>

      {% form 'contact' %}
        {% if form.posted_successfully? %}
          <div class="ai-kulkid-contact__form-success-{{ ai_gen_id }}">
            {{ block.settings.success_message }}
          </div>
        {% else %}
          <div class="ai-kulkid-contact__form-grid-{{ ai_gen_id }}">
            <div class="ai-kulkid-contact__form-field-{{ ai_gen_id }}">
              <label for="contact-name-{{ ai_gen_id }}" class="ai-kulkid-contact__form-label-{{ ai_gen_id }}">
                Navn *
              </label>
              <input
                type="text"
                id="contact-name-{{ ai_gen_id }}"
                name="contact[name]"
                class="ai-kulkid-contact__form-input-{{ ai_gen_id }}"
                value="{{ form.name }}"
                required
                aria-required="true"
              >
              {% if form.errors contains 'name' %}
                <div class="ai-kulkid-contact__form-error-{{ ai_gen_id }}">
                  {{ form.errors.messages.name }}
                </div>
              {% endif %}
            </div>

            <div class="ai-kulkid-contact__form-field-{{ ai_gen_id }}">
              <label for="contact-email-{{ ai_gen_id }}" class="ai-kulkid-contact__form-label-{{ ai_gen_id }}">
                E-post *
              </label>
              <input
                type="email"
                id="contact-email-{{ ai_gen_id }}"
                name="contact[email]"
                class="ai-kulkid-contact__form-input-{{ ai_gen_id }}"
                value="{{ form.email }}"
                required
                aria-required="true"
                autocomplete="email"
              >
