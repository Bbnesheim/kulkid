                <option value="Retur" {% if form.subject == 'Retur' %}selected{% endif %}>Retur</option>
                <option value="Feil st√∏rrelse" {% if form.subject == 'Feil st√∏rrelse' %}selected{% endif %}>Feil st√∏rrelse</option>
                <option value="Sp√∏rsm√•l om levering" {% if form.subject == 'Sp√∏rsm√•l om levering' %}selected{% endif %}>Sp√∏rsm√•l om levering</option>
                <option value="Annet" {% if form.subject == 'Annet' %}selected{% endif %}>Annet</option>
              </select>
              {% if form.errors contains 'subject' %}
                <div class="ai-kulkid-contact__form-error-{{ ai_gen_id }}">
                  {{ form.errors.messages.subject }}
                </div>
              {% endif %}
            </div>

            <div class="ai-kulkid-contact__form-field-{{ ai_gen_id }} ai-kulkid-contact__form-field--full-{{ ai_gen_id }}">
              <label for="contact-message-{{ ai_gen_id }}" class="ai-kulkid-contact__form-label-{{ ai_gen_id }}">
                Melding *
              </label>
              <textarea
                id="contact-message-{{ ai_gen_id }}"
                name="contact[body]"
                class="ai-kulkid-contact__form-textarea-{{ ai_gen_id }}"
                required
                aria-required="true"
                placeholder="Beskriv ditt sp√∏rsm√•l eller problem..."
              >{{ form.body }}</textarea>
              {% if form.errors contains 'body' %}
                <div class="ai-kulkid-contact__form-error-{{ ai_gen_id }}">
                  {{ form.errors.messages.body }}
                </div>
              {% endif %}
            </div>
          </div>

          <button type="submit" class="ai-kulkid-contact__form-button-{{ ai_gen_id }}">
