            <rect width="20" height="16" x="2" y="4" rx="2"/>
            <path d="m22 7-10 5L2 7"/>
          </svg>
          <a href="mailto:{{ block.settings.email_address }}" class="ai-kulkid-contact__email-{{ ai_gen_id }}">
            {{ block.settings.email_address }}
          </a>
        </div>
        <div class="ai-kulkid-contact__info-item-{{ ai_gen_id }}">
