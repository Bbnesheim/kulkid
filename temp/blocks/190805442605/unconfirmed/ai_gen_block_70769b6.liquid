{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-contact-page-{{ ai_gen_id }} {
    max-width: 800px;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px;
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    font-family: {{ block.settings.body_font.family }}, {{ block.settings.body_font.fallback_families }};
    font-weight: {{ block.settings.body_font.weight }};
  }

  .ai-contact-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: {{ block.settings.section_spacing }}px;
  }

  .ai-contact-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 700;
    color: {{ block.settings.title_color }};
    margin: 0 0 12px 0;
    line-height: 1.2;
    font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }};
    font-style: {{ block.settings.title_font.style }};
  }

  .ai-contact-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    font-weight: 500;
    font-family: {{ block.settings.subtitle_font.family }}, {{ block.settings.subtitle_font.fallback_families }};
    font-style: {{ block.settings.subtitle_font.style }};
  }

  .ai-contact-content-{{ ai_gen_id }} {
    display: grid;
    gap: {{ block.settings.content_spacing }}px;
  }

  .ai-contact-section-{{ ai_gen_id }} {
    position: relative;
    background-color: {{ block.settings.card_background }};
    padding: {{ block.settings.card_padding }}px;
    border-radius: {{ block.settings.card_radius }}px;
    overflow: hidden;
  }

  {% if block.settings.enable_rainbow_border %}
    .ai-contact-section-{{ ai_gen_id }}::before {
      content: '';
      position: absolute;
      top: -{{ block.settings.border_width }}px;
      left: -{{ block.settings.border_width }}px;
      right: -{{ block.settings.border_width }}px;
      bottom: -{{ block.settings.border_width }}px;
      background: linear-gradient(
        {{ block.settings.gradient_angle }}deg,
        {{ block.settings.color_1 }},
        {{ block.settings.color_2 }},
        {{ block.settings.color_3 }},
        {{ block.settings.color_4 }},
        {{ block.settings.color_5 }},
        {{ block.settings.color_6 }},
        {{ block.settings.color_7 }},
        {{ block.settings.color_8 }},
        {{ block.settings.color_9 }},
        {{ block.settings.color_10 }},
        {{ block.settings.color_11 }},
        {{ block.settings.color_12 }},
        {{ block.settings.color_13 }},
        {{ block.settings.color_14 }},
        {{ block.settings.color_1 }}
      );
      background-size: {{ block.settings.gradient_size }}% {{ block.settings.gradient_size }}%;
      border-radius: {{ block.settings.card_radius | plus: block.settings.border_width }}px;
      animation: ai-rainbow-flow-{{ ai_gen_id }} {{ block.settings.animation_speed }}s linear infinite;
      z-index: 1;
      filter: blur({{ block.settings.blur_amount }}px) brightness({{ block.settings.brightness_level }}%) saturate({{ block.settings.saturation_level }}%) contrast({{ block.settings.contrast_level }}%);
      opacity: {{ block.settings.border_opacity }};
    }

    .ai-contact-section-{{ ai_gen_id }}::after {
      content: '';
      position: absolute;
      top: {{ block.settings.border_width | times: block.settings.inner_border_offset }}px;
      left: {{ block.settings.border_width | times: block.settings.inner_border_offset }}px;
      right: {{ block.settings.border_width | times: block.settings.inner_border_offset }}px;
      bottom: {{ block.settings.border_width | times: block.settings.inner_border_offset }}px;
      background-color: {{ block.settings.card_background }};
      border-radius: {{ block.settings.card_radius }}px;
      z-index: 2;
    }

    .ai-contact-section-{{ ai_gen_id }} > * {
      position: relative;
      z-index: 3;
    }

    @keyframes ai-rainbow-flow-{{ ai_gen_id }} {
      0% {
        background-position: 0% 50%;
      }
      25% {
        background-position: 100% 100%;
      }
      50% {
        background-position: 200% 50%;
      }
      75% {
        background-position: 100% 0%;
      }
      100% {
        background-position: 0% 50%;
      }
    }

    {% if block.settings.enable_glow_effect %}
      .ai-contact-section-{{ ai_gen_id }} {
        box-shadow: 
          0 0 {{ block.settings.glow_size }}px {{ block.settings.glow_color_1 }},
          0 0 {{ block.settings.glow_size | times: 1.5 }}px {{ block.settings.glow_color_2 }},
          0 0 {{ block.settings.glow_size | times: 2 }}px {{ block.settings.glow_color_3 }};
        animation: ai-glow-pulse-{{ ai_gen_id }} {{ block.settings.glow_speed }}s ease-in-out infinite alternate;
      }

      @keyframes ai-glow-pulse-{{ ai_gen_id }} {
        0% {
          box-shadow: 
            0 0 {{ block.settings.glow_size }}px {{ block.settings.glow_color_1 }},
            0 0 {{ block.settings.glow_size | times: 1.5 }}px {{ block.settings.glow_color_2 }},
            0 0 {{ block.settings.glow_size | times: 2 }}px {{ block.settings.glow_color_3 }};
        }
        100% {
          box-shadow: 
            0 0 {{ block.settings.glow_size | times: block.settings.glow_intensity }}px {{ block.settings.glow_color_1 }},
            0 0 {{ block.settings.glow_size | times: 1.5 | times: block.settings.glow_intensity }}px {{ block.settings.glow_color_2 }},
            0 0 {{ block.settings.glow_size | times: 2 | times: block.settings.glow_intensity }}px {{ block.settings.glow_color_3 }};
        }
      }
    {% endif %}

    {% if block.settings.enable_border_pulse %}
      .ai-contact-section-{{ ai_gen_id }}::before {
        animation: 
          ai-rainbow-flow-{{ ai_gen_id }} {{ block.settings.animation_speed }}s linear infinite,
          ai-border-pulse-{{ ai_gen_id }} {{ block.settings.pulse_speed }}s ease-in-out infinite alternate;
      }

      @keyframes ai-border-pulse-{{ ai_gen_id }} {
        0% {
          transform: scale(1);
          filter: blur({{ block.settings.blur_amount }}px) brightness({{ block.settings.brightness_level }}%) saturate({{ block.settings.saturation_level }}%) contrast({{ block.settings.contrast_level }}%);
        }
        100% {
          transform: scale({{ block.settings.pulse_scale }});
          filter: blur({{ block.settings.blur_amount | times: block.settings.pulse_blur_multiplier }}px) brightness({{ block.settings.brightness_level | times: block.settings.pulse_brightness_multiplier }}%) saturate({{ block.settings.saturation_level | times: block.settings.pulse_saturation_multiplier }}%) contrast({{ block.settings.contrast_level | times: block.settings.pulse_contrast_multiplier }}%);
        }
      }
    {% endif %}
  {% else %}
    .ai-contact-section-{{ ai_gen_id }} {
      border: {{ block.settings.border_width }}px solid {{ block.settings.static_border_color }};
    }
  {% endif %}

  .ai-contact-section-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.section_title_size }}px;
    font-weight: 600;
    color: {{ block.settings.section_title_color }};
