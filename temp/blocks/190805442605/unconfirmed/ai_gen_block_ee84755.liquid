  .ai-floating-icon-{{ ai_gen_id }}:nth-child(16) { top: 40%; left: 25%; animation-delay: 4.8s; }
  .ai-floating-icon-{{ ai_gen_id }}:nth-child(17) { bottom: 80%; right: 15%; animation-delay: 6.2s; }
  .ai-floating-icon-{{ ai_gen_id }}:nth-child(18) { top: 70%; left: 60%; animation-delay: 2.8s; }

  @keyframes ai-float-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0px) rotate(0deg) scale(1); }
    25% { transform: translateY(-15px) rotate(90deg) scale(1.1); }
    50% { transform: translateY(-25px) rotate(180deg) scale(0.9); }
    75% { transform: translateY(-10px) rotate(270deg) scale(1.05); }
  }

  .ai-emoji-icon-{{ ai_gen_id }} {
    font-size: 20px;
    margin-right: 6px;
  }

  .ai-emoji-large-{{ ai_gen_id }} {
    font-size: 24px;
    margin-right: 8px;
  }

  .ai-kid-icons-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    gap: 4px;
  }

  .ai-sparkle-{{ ai_gen_id }} {
    animation: ai-sparkle-{{ ai_gen_id }} 2s ease-in-out infinite;
  }

  @keyframes ai-sparkle-{{ ai_gen_id }} {
    0%, 100% { opacity: 0.6; transform: scale(1); }
    50% { opacity: 1; transform: scale(1.2); }
  }

  @media screen and (max-width: 768px) {
    .ai-contact-grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 24px;
    }

    .ai-form-row-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 16px;
    }

    .ai-contact-page-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding | times: 0.6 }}px;
    }

    .ai-contact-main-content-{{ ai_gen_id }}, .ai-contact-form-section-{{ ai_gen_id }} {
      padding: 24px;
    }

    .ai-contact-info-card-{{ ai_gen_id }}, .ai-company-info-card-{{ ai_gen_id }} {
      padding: 20px;
    }

    .ai-contact-title-{{ ai_gen_id }}, .ai-contact-subtitle-{{ ai_gen_id }} {
      flex-direction: column;
      text-align: center;
    }

    .ai-floating-icons-{{ ai_gen_id }} {
      display: none;
    }
  }
{% endstyle %}

