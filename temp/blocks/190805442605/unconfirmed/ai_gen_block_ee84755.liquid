  @keyframes ai-rainbow-border-{{ ai_gen_id }} {
    0% { background-position: 0% 50%; }
    50% { background-position: 100% 50%; }
    100% { background-position: 0% 50%; }
  }

  @keyframes ai-twinkle-{{ ai_gen_id }} {
    0%, 100% { opacity: 1; transform: translateY(-50%) scale(1); }
    50% { opacity: 0.5; transform: translateY(-50%) scale(1.3); }
  }

  @keyframes ai-bounce-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-8px); }
  }

  @keyframes ai-spin-slow-{{ ai_gen_id }} {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
  }

  @keyframes ai-float-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-12px); }
  }

  @keyframes ai-float-left-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0) translateX(0); }
    50% { transform: translateY(-18px) translateX(8px); }
  }

  @keyframes ai-float-right-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0) translateX(0); }
    50% { transform: translateY(-15px) translateX(-8px); }
  }

  @keyframes ai-float-gentle-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
  }

  @keyframes ai-pulse-{{ ai_gen_id }} {
    0%, 100% { transform: scale(1); }
    50% { transform: scale(1.15); }
  }

  @keyframes ai-pulse-gentle-{{ ai_gen_id }} {
    0%, 100% { transform: scale(1); }
    50% { transform: scale(1.08); }
  }

  @keyframes ai-glow-{{ ai_gen_id }} {
    0%, 100% { opacity: 1; }
    50% { opacity: 0.6; }
  }

  @keyframes ai-wiggle-{{ ai_gen_id }} {
    0%, 100% { transform: rotate(0deg); }
    25% { transform: rotate(8deg); }
    75% { transform: rotate(-8deg); }
  }

  @keyframes ai-celebrate-{{ ai_gen_id }} {
    0%, 100% { transform: scale(1) rotate(0deg); }
    50% { transform: scale(1.3) rotate(15deg); }
  }

  @keyframes ai-float-random-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0) translateX(0); }
    25% { transform: translateY(-20px) translateX(12px); }
    50% { transform: translateY(-12px) translateX(-8px); }
    75% { transform: translateY(-25px) translateX(5px); }
  }

  @media screen and (min-width: 768px) {
    .ai-contact-grid-{{ ai_gen_id }} {
      grid-template-columns: 2fr 1fr;
      gap: 60px;
    }

    .ai-contact-cards-{{ ai_gen_id }} {
      gap: 40px;
    }

    .ai-contact-form-row-{{ ai_gen_id }} {
      grid-template-columns: 1fr 1fr;
    }
  }

  @media screen and (max-width: 767px) {
    .ai-contact-page-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding | divided_by: 2 }}px;
    }

    .ai-contact-page-{{ ai_gen_id }}::before,
    .ai-contact-page-{{ ai_gen_id }}::after {
      display: none;
    }

    .ai-contact-title-{{ ai_gen_id }}::before,
    .ai-contact-title-{{ ai_gen_id }}::after {
      display: none;
    }

    .ai-contact-main-content-{{ ai_gen_id }},
    .ai-contact-card-{{ ai_gen_id }},
    .ai-contact-form-section-{{ ai_gen_id }} {
      padding: 24px;
    }

    .ai-contact-form-row-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }

    .ai-contact-main-text-{{ ai_gen_id }} {
      padding-left: 0;
    }

    .ai-contact-main-text-{{ ai_gen_id }}::before {
      display: none;
    }

    .ai-contact-form-subtitle-{{ ai_gen_id }} {
      padding-left: 0;
    }

    .ai-contact-form-subtitle-{{ ai_gen_id }}::before {
      display: none;
    }

    .ai-floating-icons-{{ ai_gen_id }} {
      display: none;
    }

    .ai-contact-main-title-{{ ai_gen_id }}::after {
      display: none;
    }

    .ai-contact-form-title-{{ ai_gen_id }}::after {
      display: none;
    }

    .ai-contact-subtitle-{{ ai_gen_id }}::before,
    .ai-contact-subtitle-{{ ai_gen_id }}::after {
      display: none;
    }
  }
{% endstyle %}

