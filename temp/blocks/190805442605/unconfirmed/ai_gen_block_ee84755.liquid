        <div class="ai-contact-form-row-{{ ai_gen_id }}">
          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="contact_name_{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              <span>👤🎭</span>
              Navn *
            </label>
            <input
              type="text"
              id="contact_name_{{ ai_gen_id }}"
              name="contact[name]"
              class="ai-contact-form-input-{{ ai_gen_id }}"
              value="{{ form.name }}"
              required
              placeholder="Ditt navn"
            >
          </div>
          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="contact_email_{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              <span>📨💌</span>
              E-post *
            </label>
            <input
              type="email"
              id="contact_email_{{ ai_gen_id }}"
              name="contact[email]"
              class="ai-contact-form-input-{{ ai_gen_id }}"
              value="{{ form.email }}"
              required
              placeholder="din@epost.no"
            >
          </div>
        </div>

        <div class="ai-contact-form-row-{{ ai_gen_id }}">
          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="contact_order_{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              <span>📦🎁</span>
              Ordrenummer (valgfritt)
            </label>
            <input
              type="text"
              id="contact_order_{{ ai_gen_id }}"
              name="contact[order_number]"
              class="ai-contact-form-input-{{ ai_gen_id }}"
              placeholder="#12345"
            >
          </div>
          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="contact_subject_{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              <span>🎯🎪</span>
              Velg emne *
            </label>
            <select
              id="contact_subject_{{ ai_gen_id }}"
              name="contact[subject]"
              class="ai-contact-form-select-{{ ai_gen_id }}"
              required
            >
              <option value="">Velg emne...</option>
              <option value="Retur">🔄🎈 Retur</option>
              <option value="Feil størrelse">📏🎭 Feil størrelse</option>
              <option value="Spørsmål om levering">🚚🌟 Spørsmål om levering</option>
              <option value="Annet">🤔🎨 Annet</option>
            </select>
          </div>
        </div>

        <div class="ai-contact-form-field-{{ ai_gen_id }}">
          <label for="contact_message_{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
            <span>💬🎪</span>
            Melding *
          </label>
          <textarea
            id="contact_message_{{ ai_gen_id }}"
            name="contact[body]"
            class="ai-contact-form-textarea-{{ ai_gen_id }}"
            required
            placeholder="Beskriv hvordan vi kan hjelpe deg..."
          >{{ form.body }}</textarea>
        </div>
      </div>

      <button type="submit" class="ai-contact-form-button-{{ ai_gen_id }}">
        <span>🚀🎪</span>
        {{ block.settings.form_button_text }}
        <span>🎈🌟</span>
      </button>

      {%- if form.posted_successfully? -%}
        <div class="ai-contact-form-success-{{ ai_gen_id }}">
          <span>💌🎉</span>
          {{ block.settings.form_success_message }}
        </div>
      {%- endif -%}

      {%- if form.errors -%}
        <div class="ai-contact-form-error-{{ ai_gen_id }}">
          <span>⚠️🎭</span>
          {{ form.errors | default_errors }}
        </div>
      {%- endif -%}
    {%- endform -%}
  </div>
</div>

