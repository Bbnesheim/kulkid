            {{ block.settings.company_address }}
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="ai-contact-form-section-{{ ai_gen_id }}">
    {%- form 'contact' -%}
      <h2 class="ai-contact-form-title-{{ ai_gen_id }}">
        <span>🎪🎨🧸</span>
        {{ block.settings.form_title }}
      </h2>
      <p class="ai-contact-form-subtitle-{{ ai_gen_id }}">{{ block.settings.form_subtitle }}</p>

