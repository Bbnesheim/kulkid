    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
    <span class="ai-floating-icon-{{ ai_gen_id }}"></span>
