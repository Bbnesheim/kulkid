          <div class="ai-contact-info-content-{{ ai_gen_id }}">
            <p class="ai-contact-info-label-{{ ai_gen_id }}">Åpningstider</p>
            <p class="ai-contact-info-value-{{ ai_gen_id }}">{{ block.settings.opening_hours }}</p>
