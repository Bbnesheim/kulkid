            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z" />
          </svg>
          <div class="ai-contact-info-content-{{ ai_gen_id }}">
            <p class="ai-contact-info-label-{{ ai_gen_id }}">Organisasjonsnummer</p>
            <p class="ai-contact-info-value-{{ ai_gen_id }}">{{ block.settings.org_number }}</p>
          </div>
        </div>

        <div class="ai-contact-info-item-{{ ai_gen_id }}">
          <svg class="ai-contact-info-icon-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
          </svg>
          <div class="ai-contact-info-content-{{ ai_gen_id }}">
            <p class="ai-contact-info-label-{{ ai_gen_id }}">Adresse</p>
            <p class="ai-contact-info-value-{{ ai_gen_id }}">{{ block.settings.company_address }}</p>
          </div>
        </div>
      </div>
    </div>

    <div class="ai-contact-form-{{ ai_gen_id }}">
      <h2 class="ai-contact-form-title-{{ ai_gen_id }}">{{ block.settings.form_title }}</h2>
      <p class="ai-contact-form-description-{{ ai_gen_id }}">{{ block.settings.form_description }}</p>
      
      <div class="ai-contact-form-wrapper-{{ ai_gen_id }}">
        {%- form 'contact' -%}
          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="ContactForm-name-{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              Navn *
            </label>
            <input
              class="ai-contact-form-input-{{ ai_gen_id }}"
              autocomplete="name"
              type="text"
              id="ContactForm-name-{{ ai_gen_id }}"
              name="contact[name]"
              value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}"
              placeholder="Ditt fulle navn"
              required
            >
          </div>

          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="ContactForm-email-{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              E-post *
            </label>
            <input
              autocomplete="email"
              type="email"
              id="ContactForm-email-{{ ai_gen_id }}"
              class="ai-contact-form-input-{{ ai_gen_id }}"
              name="contact[email]"
              spellcheck="false"
              autocapitalize="off"
              value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
              aria-required="true"
              {% if form.errors contains 'email' %}
                aria-invalid="true"
                aria-describedby="ContactForm-email-error-{{ ai_gen_id }}"
              {% endif %}
              placeholder="din@epost.no"
              required
            >
            {%- if form.errors contains 'email' -%}
              <div id="ContactForm-email-error-{{ ai_gen_id }}" class="ai-contact-form-error-{{ ai_gen_id }}">
                <span>{{ form.errors.messages.email }}</span>
              </div>
            {%- endif -%}
          </div>

          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="ContactForm-order-{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              Ordrenummer (valgfritt)
            </label>
            <input
              type="text"
              id="ContactForm-order-{{ ai_gen_id }}"
              class="ai-contact-form-input-{{ ai_gen_id }}"
              name="contact[order_number]"
              value="{{ form.order_number }}"
              placeholder="#1234"
            >
          </div>

          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="ContactForm-subject-{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              Velg emne *
            </label>
            <select
              id="ContactForm-subject-{{ ai_gen_id }}"
              class="ai-contact-form-select-{{ ai_gen_id }}"
              name="contact[subject]"
              required
            >
              <option value="">Velg et emne</option>
              <option value="Retur" {% if form.subject == 'Retur' %}selected{% endif %}>Retur</option>
              <option value="Feil st√∏rrelse" {% if form.subject == 'Feil st√∏rrelse' %}selected{% endif %}>Feil st√∏rrelse</option>
              <option value="Sp√∏rsm√•l om levering" {% if form.subject == 'Sp√∏rsm√•l om levering' %}selected{% endif %}>Sp√∏rsm√•l om levering</option>
              <option value="Annet" {% if form.subject == 'Annet' %}selected{% endif %}>Annet</option>
            </select>
          </div>

          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <label for="ContactForm-body-{{ ai_gen_id }}" class="ai-contact-form-label-{{ ai_gen_id }}">
              Melding *
            </label>
            <textarea
              rows="10"
              id="ContactForm-body-{{ ai_gen_id }}"
              class="ai-contact-form-textarea-{{ ai_gen_id }}"
              name="contact[body]"
              placeholder="Beskriv ditt sp√∏rsm√•l eller problem..."
              required
            >
              {{- form.body -}}
            </textarea>
          </div>

          <div class="ai-contact-form-field-{{ ai_gen_id }}">
            <button type="submit" class="ai-contact-form-button-{{ ai_gen_id }}">
              Send melding
            </button>
          </div>

          {%- if form.posted_successfully? -%}
            <div class="ai-contact-form-success-{{ ai_gen_id }}">
              {{ block.settings.success_message }}
            </div>
          {%- endif -%}

          {%- if form.errors -%}
            <div class="ai-contact-form-error-{{ ai_gen_id }}">
              <h3>Det oppstod en feil:</h3>
              <ul>
                {%- for field in form.errors -%}
                  <li>
                    {%- if field == 'form' -%}
                      {{ form.errors.messages[field] }}
                    {%- else -%}
                      {{ form.errors.messages[field] }}
                    {%- endif -%}
                  </li>
                {%- endfor -%}
              </ul>
            </div>
          {%- endif -%}
        {%- endform -%}
      </div>
    </div>
  </div>
</div>

